{
    "routes": [
      { "handle": "filesystem" },
  
      { "src": "/app-ads.txt", "dest": "/app-ads.txt" },
  
      { "src": "/(.*)", "dest": "/index.html" }
    ],
    "headers": [
      {
        "source": "/app-ads.txt",
        "headers": [
          { "key": "Content-Type", "value": "text/plain; charset=utf-8" }
        ]
      }
    ]
  }
  